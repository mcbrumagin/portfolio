# Example Terraform variables file
# Copy this to terraform.tfvars and customize for your environment

# AWS region
aws_region = "us-east-1"

# Project configuration
environment  = "dev"  # or "prod"

# Domain configuration
# Make sure this domain is already registered and has a Route53 hosted zone
portfolio_domain_name = "mcbrumagin.com"
soundclone_domain_name = "soundcl.one"

# SSL configuration
enable_ssl = true
